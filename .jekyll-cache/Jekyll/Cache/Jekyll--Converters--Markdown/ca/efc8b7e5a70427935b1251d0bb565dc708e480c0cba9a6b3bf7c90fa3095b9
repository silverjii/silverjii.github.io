<figure class="highlight"><pre><code class="language-javascript" data-lang="javascript"> <span class="kd">const</span> <span class="nx">browser</span> <span class="o">=</span> <span class="dl">'</span><span class="s1">IE</span><span class="dl">'</span><span class="p">;</span>
 <span class="k">switch</span> <span class="p">(</span><span class="nx">browser</span><span class="p">){</span>
     <span class="k">case</span> <span class="dl">'</span><span class="s1">IE</span><span class="dl">'</span><span class="p">:</span>
        <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="dl">'</span><span class="s1">go away!</span><span class="dl">'</span><span class="p">);</span> <span class="c1">// go away ì¶œë ¥</span>
        <span class="k">break</span><span class="p">;</span>
    
    <span class="k">case</span> <span class="dl">'</span><span class="s1">Chrome</span><span class="dl">'</span><span class="p">:</span>
    <span class="k">case</span> <span class="dl">'</span><span class="s1">Firefox</span><span class="dl">'</span><span class="p">:</span>   <span class="c1">// Chrome, Firefox ì¼ ë•Œ ì‹¤í–‰</span>
        <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="dl">'</span><span class="s1">love you!</span><span class="dl">'</span><span class="p">);</span>
        <span class="k">break</span><span class="p">;</span>
    <span class="nl">default</span><span class="p">:</span>
        <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="dl">'</span><span class="s1">same all!</span><span class="dl">'</span><span class="p">);</span>
